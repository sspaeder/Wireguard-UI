<Window x:Class="WireGuard.GUI.Windows.ImportWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WireGuard.GUI.Windows"
        xmlns:pages="clr-namespace:WireGuard.GUI.Pages"
        xmlns:vm="clr-namespace:WireGuard.GUI.ViewModels"
        d:DataContext="{d:DesignInstance Type=vm:ImportViewModel}"
        mc:Ignorable="d"
        Title="ImportWindow" Height="350" Width="350"
        DataContextChanged="Window_DataContextChanged">
    <Grid>
        <!-- Second frame for displaying a page -->
        <Frame x:Name="frame1" 
               Background="{x:Null}"
               NavigationUIVisibility="Hidden">
        </Frame>
        
        <!-- First frame for displaying a page -->
        <Frame x:Name="frame2" 
               Background="{x:Null}"
               NavigationUIVisibility="Hidden">
        </Frame>
    </Grid>
</Window>
